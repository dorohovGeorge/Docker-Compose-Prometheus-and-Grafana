{
    "family": "prometheus",
    "containerDefinitions": [
        {
            "portMappings": [
                {
                    "hostPort": 7100,
                    "containerPort": 7100,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "name": "node_exporter",
            "image": "prom/node-exporter",
            "cpu": 0,
            "privileged": null,
            "memoryReservation": 150
        }
    ],
    "volumes": [],
    "networkMode": "host"
}
